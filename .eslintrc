{
	"env": {
		"node": true
	},
	"rules": {
		"no-multi-spaces": [2, { "exceptions": { "VariableDeclarator": true } }],
		"no-use-before-define": [2, "nofunc"],
		"key-spacing": [2, { "align": "value" }],
		"eqeqeq": [0, "smart"],
		"quotes": [1, "single", "avoid-escape"],
		"curly": [2, "multi"],
		"no-underscore-dangle": 0
	}
}
